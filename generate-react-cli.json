{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "style": "templates/default/TemplateName.module.scss",
        "story": "templates/default/TemplateName.stories.ts"
      },
      "path": "src/shared/ui",
      "withLazy": false,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/pages/TemplateName.tsx",
        "style": "templates/pages/TemplateName.module.scss",
        "story": "templates/pages/TemplateName.stories.ts",
        "lazy":  "templates/pages/TemplateName.lazy.ts"
      },
      "path": "src/pages",
      "withLazy": true,
      "withStory": true,
      "withStyle": true,
      "withTest": false
    }
    


  },
  "usesStyledComponents": false
}